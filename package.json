{
  "name": "upzone",
  "version": "1.0.9",
  "description": "A file upload component with drag-and-drop functionality. Provides easy integration for file selection, validation, and progress tracking.",
  "type": "module",
  "main": "./dist/upzone.js",
  "module": "./dist/upzone.esm.js",
  "types": "./dist/upzone.d.ts",
  "style": "./dist/upzone.css",
  "exports": {
    ".": {
      "import": "./dist/upzone.esm.js",
      "require": "./dist/upzone.js"
    },
    "./css": "./dist/upzone.css",
    "./vue": "./dist/upzone.vue.js"
  },
  "license": "MIT",
  "keywords": [
    "dropzone",
    "upload",
    "file upload",
    "upload files",
    "multiples files",
    "javascript",
    "vuejs",
    "typescript",
    "upzone",
    "vuedropzone"
  ],
  "author": "Gustavo Silva <gug.contato@gmail.com>",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/gitgusilva/upzone/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitgusilva/upzone.git"
  },
  "directories": {
    "src": "src/"
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.83.0",
    "tslib": "^2.8.1",
    "vue": "^3.5.13"
  }
}